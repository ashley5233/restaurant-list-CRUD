{{!-- card --}}
<div class="container">
  <div class="row">
    {{#each restaurant}}
    <div class="col-sm-4 mt-6">
      <div class="card" style="width: 18rem;">
        <img src="{{this.image}}" class="card-img-top" alt="image">
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>